<!DOCTYPE html>
<html>
  <head>
    <title>Naganocake</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
      <nav class='navbar navbar-expand-md  border-bottom ' style='height: 130px'>
        <div class='container'>
          <p class="navbar-brand text-dark" >NAGANO CAKE</p>
          <div>
            <ul class="navbar-nav ml-auto">
                <li><%= link_to '商品一覧', admin_items_path, class:'nav-link text-dark' %></li>
                <li><%= link_to '会員一覧', admin_customers_path, class:'nav-link text-dark' %></li>
                <li><%= link_to '注文履歴一覧', admin_orders_path, class:'nav-link text-dark' %></li>
                <li><%= link_to 'ジャンル一覧', admin_genres_path, class:'nav-link text-dark' %></li>
                <li><%= link_to 'ログアウト', destroy_admin_session_path, method: :delete, class:'nav-link text-dark' %></li>
            </ul>
          </div>
        </div>

      </nav>
    </header>
    <main>
      <%= yield %>
    </main>
  </body>
</html>
